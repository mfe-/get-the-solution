{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "dceb72e9-b0bc-4b48-82b2-d2a09ceceeea"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "get-the-solution-app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/get-the-solution-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              { "input": "src/styles.css", "inject": true }
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "assets" : [
                { "glob": "**/*.json", "input": "src/assets/", "output": "/assets/" },
                { "glob": "**/*.html", "input": "src/assets/", "output": "/assets/" },
                { "glob": "**/*.html", "input": "src/assets/", "output": "/assets/" },
                { "glob": "**/*.jpg", "input": "src/assets/", "output": "/assets/" },
                { "glob": "**/*.png", "input": "src/assets/", "output": "/assets/" },
                { "glob": "**/*.gif", "input": "src/assets/", "output": "/assets/" },
                { "glob": "**/*.svg", "input": "src/assets/", "output": "/assets/" },
                { "glob": "favicon.ico", "input": "src/", "output": "./" },
                { "glob": "404.html", "input": "src/", "output": "./" },
                { "glob": "CNAME", "input": "./", "output": "./" },
                { "glob": "ads.html", "input": "src/", "output": "./" },
                { "glob": "src/robots.txt", "input": "src/", "output": "./" },
                { "glob": "src/sitemap.xml", "input": "src/", "output": "./" }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "get-the-solution-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "get-the-solution-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "get-the-solution-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              { "input": "src/styles.css", "inject": true }
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        }
      }
    },
    "get-the-solution-app-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "get-the-solution-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "get-the-solution-app:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "get-the-solution-app"
}